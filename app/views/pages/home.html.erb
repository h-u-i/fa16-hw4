<b>Cat Model</b>
<table>
  <tr>
    <td>cat name</td>
  </tr>
  <% @cats.each do |cat| %>
    <tr>
      <td><%= cat.name%></td>
    </tr>
  <% end %>
</table>
<b> Todo Model </b>
<table>
  <tr>
      <td>created_at</td>
      <td>updated_at</td>
  </tr>
  <% @todos.each do |todo| %>
    <tr>
        <td><%= todo.created_at%></td>
        <td><%= todo.updated_at%></td>
    </tr>
  <% end %>
</table>
<b> User Model</b>
<table>
    <tr>
        <td> username </td>
        <td> email </td>
        <td> created_at </td>
        <td> updated_at </td>
        <td> age </td>
    </tr>
    <% @users.each do |user| %>
    <tr>
        <td><%= user.username%></td>
        <td><%= user.email%></td>
        <td><%= user.created_at%> </td>
        <td><%= user.updated_at%> </td>
        <td><%= user.age%></td>
    </tr>
    <%end%>
</table>
